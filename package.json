{
  "private": true,
  "name": "ditt-client",
  "version": "0.1.0",
  "description": "DBJR Internal Time Tracking application developed by VisionApps",
  "main": "src/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "test": "stylelint \"src/**/*.scss\" \"public/css/*.css\" --config stylelint.config.js && eslint --ext js,jsx src && jest"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "<rootDir>/tests/setupJest.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setupEnzyme.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.17",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-immutable": "^0.1.3",
    "react-immutable-proptypes": "^2.1.0",
    "react-pointable": "^1.1.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-ui": "git+https://bitbucket.visionapps.cz/scm/va/react-ui.git#develop",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.3.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "validator": "^9.4.1",
    "webpack": "^4.8.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-visionapps": "^0.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^7.0.1",
    "sinon": "^4.4.9",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-visionapps": "^0.5.0",
    "stylelint-config-visionapps-order": "^0.3.1",
    "stylelint-order": "^0.8.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
